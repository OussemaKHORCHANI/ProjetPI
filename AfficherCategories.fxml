<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.AfficherCategoriesController">
    <children>
      <ImageView fitHeight="48.0" fitWidth="46.0" layoutX="447.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../icons8-ajouter-un-marque-page-100_1.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="490.0" layoutY="39.0" prefHeight="23.0" prefWidth="180.0" text=" list of categories" textFill="#9a8e8e">
         <font>
            <Font name="Bauhaus 93" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tabc" layoutX="504.0" layoutY="98.0" prefHeight="325.0" prefWidth="473.0">
         <columns>
            <TableColumn fx:id="TextType" prefWidth="101.0" text="Type" />
         </columns>
      </TableView>
      <Button layoutX="504.0" layoutY="476.0" mnemonicParsing="false" onAction="#AddCategoriesAction" prefHeight="19.0" prefWidth="42.0" style="-fx-background-color: #f99f1b; -fx-background-radius: 7em;" text="Add">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <Button layoutX="565.0" layoutY="476.0" mnemonicParsing="false" onAction="#UpdateCategoriesAction" style="-fx-background-color: #f99f1b; -fx-background-radius: 7em;" text="Update">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
      <Button layoutX="623.0" layoutY="476.0" mnemonicParsing="false" onAction="#deleteCategoriesAction" style="-fx-background-color: #f99f1b; -fx-background-radius: 7em;" text="Delete">
         <font>
            <Font size="9.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
